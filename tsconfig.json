{
  "exclude": ["lib/__tests__", "lib/__fixtures__"],
  "include": ["lib/", "declarations.d.ts", "dist/declarations.d.ts"],
  "compilerOptions": {
    "module": "ES6",
    "target": "ES6",
    "outDir": "./dist",
    "sourceMap": true,
    "esModuleInterop": true,
    "declaration": true,
    "skipLibCheck": true,                     
    "forceConsistentCasingInFileNames": true   
  }
}
