{
  "name": "@freecodecamp/curriculum-helpers",
  "version": "1.1.0",
  "description": "Helper functions to test challenges in freeCodecamp's curriculum",
  "homepage": "https://freecodecamp.org",
  "author": {
    "name": "freeCodeCamp.org",
    "email": "team@freecodecamp.org",
    "url": "https://freecodecamp.org"
  },
  "files": [
    "/dist"
  ],
  "main": "dist/",
  "keywords": [
    ""
  ],
  "devDependencies": {
    "@babel/core": "7.22.17",
    "@babel/preset-env": "7.22.15",
    "@babel/preset-typescript": "7.22.15",
    "@babel/types": "7.22.17",
    "@types/jest": "27.5.2",
    "bun": "1.0.1",
    "chai": "4.3.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-xo": "0.27.2",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "8.0.1",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "29.7.0",
    "jest-ts-webcompat-resolver": "1.0.0",
    "lint-staged": "9.5.0",
    "prettier": "2.8.8"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "resolver": "jest-ts-webcompat-resolver",
    "transformIgnorePatterns": [
      "/node_modules/(?!babeliser)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "build": "bun build lib/index.ts --outdir dist --minify --sourcemap=external && tsc",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "type": "module",
  "repository": "git@github.com:freeCodeCamp/curriculum-helpers.git",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@babel/generator": "7.22.15",
    "@babel/parser": "7.22.16",
    "browserify": "^17.0.0"
  }
}
