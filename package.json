{
  "name": "@freecodecamp/curriculum-helpers",
  "version": "1.0.5",
  "description": "Helper functions to test challenges in freeCodecamp's curriculum",
  "homepage": "https://freecodecamp.org",
  "author": {
    "name": "freeCodeCamp.org",
    "email": "team@freecodecamp.org",
    "url": "https://freecodecamp.org"
  },
  "files": [
    "/dist"
  ],
  "//main": "dist/",
  "keywords": [
    ""
  ],
  "devDependencies": {
    "@babel/core": "7.19.6",
    "@babel/preset-env": "7.19.4",
    "@babel/preset-typescript": "7.18.6",
    "@types/jest": "27.5.2",
    "chai": "4.3.6",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-config-xo": "0.27.2",
    "eslint-plugin-prettier": "3.4.1",
    "husky": "8.0.1",
    "jest": "26.6.3",
    "jest-ts-webcompat-resolver": "1.0.0",
    "lint-staged": "9.5.0",
    "logover": "2.0.0",
    "mocha": "10.1.0",
    "prettier": "2.7.1",
    "snowpack": "^3.8.8",
    "tsdx": "^0.14.1",
    "vite": "^3.2.2",
    "@size-limit/preset-small-lib": "^8.1.0",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.1"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "build:vite": "vite build",
    "build": "snowpack build",
    "build:snowpack": "snowpack build",
    "pretest": "npm run build",
    "test": "jest",
    "prepublishOnly": "tsc",
    "test:babeliser": "mocha -r ts-node/register lib/babeliser/*.test.ts",
    "test:vite": "vite build ./dist/babeliser/index.test.ts",
    "test:snowpack": "NODE_ENV=test npm run build:snowpack && mocha snowpack/babeliser/index.test.js",
    "start": "tsdx watch",
    "build:tsdx": "tsdx build",
    "test:tsdx": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "repository": "git@github.com:freeCodeCamp/curriculum-helpers.git",
  "jest": {
    "resolver": "jest-ts-webcompat-resolver"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "browserify": "^17.0.0"
  },
  "peerDependencies": {
    "@babel/generator": "7.X",
    "@babel/parser": "7.X",
    "@babel/types": "7.X"
  },
  "size-limit": [
    {
      "path": "dist/curriculum-helpers.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/curriculum-helpers.esm.js",
      "limit": "10 KB"
    }
  ],
  "module": "dist/curriculum-helpers.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts"
}
